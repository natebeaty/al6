{
  "require": {
    "craftcms/cms": "3.7.38",
    "craftcms/redactor": "2.10.5",
    "elivz/craft-single-cat": "1.2.1",
    "mmikkel/cp-field-inspect": "1.4.0.1",
    "samhernandez/craft-shortcodes": "1.1.0",
    "spicyweb/craft-embedded-assets": "2.10.3",
    "verbb/expanded-singles": "1.2.0",
    "verbb/super-table": "2.7.1",
    "vlucas/phpdotenv": "^5.4.0",
    "wbrowar/adminbar": "3.1.11"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.4"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
