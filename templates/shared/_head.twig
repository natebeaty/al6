<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta http-equiv="X-UA-Compatible" content="IE=Edge">
  <title>{% if seoTitle is defined %}{{ seoTitle }}{% elseif title is defined %}{{ title }} | {{ siteName }}{% else %}{{ siteName }}{% endif %}</title>
  {% if seoDescription is defined -%}
  <meta name="description" content="{{ seoDescription }}">
  {% endif -%}
  <link rel="canonical" href="{{ siteUrl }}">
  <link rel="stylesheet" media="screen" href="/assets/styles/main.css?ckcachebust=766631802">
  <link rel="stylesheet" media="print" href="/assets/styles/print.css?ckcachebust=766631802">
  <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png?ckcachebust=766631802">
  <link rel="icon" href="/favicon.ico?v=2022&ckcachebust=766631802">

  <meta property="og:title" content="{% if seoTitle is defined %}{{ seoTitle }}{% elseif title is defined %}{{ title }} | {{ siteName }}{% else %}{{ siteName }}{% endif %}">

  {% if seoDescription is defined -%}
  <meta property="og:description" content="{{ seoDescription }}">
  {% endif -%}
  {% if seoImage is defined -%}
  <meta property="og:image" content="{{ seoImage.getUrl() }}">
  {% elseif siteOptions.shareImage|length -%}
  <meta property="og:image" content="{{ siteOptions.shareImage.one().getUrl() }}">
  {% endif -%}

  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com?ckcachebust=3D55E1E012D3824E53E84D404A6E2F2E" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Bitter:wght@800&family=Open+Sans:wght@400;700&display=swap" rel="stylesheet">

  <script>
    (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
    (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
    m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
    })(window,document,'script','//www.google-analytics.com/analytics.js','ga');

    ga('create', 'UA-3079065-1', 'www.alec-longstreth.com');
    ga('send', 'pageview');
  </script>
</head>
